/dts-v1/;

/ {
	#address-cells = <0x1>;
	#size-cells = <0x1>;
	compatible = "rockchip,rk3288-miniarm", "rockchip,rk3288";
	interrupt-parent = <0x1>;

	chosen {
	};

	aliases {
		i2c0 = "/i2c@ff650000";
		i2c1 = "/i2c@ff140000";
		i2c2 = "/i2c@ff660000";
		i2c3 = "/i2c@ff150000";
		i2c4 = "/i2c@ff160000";
		i2c5 = "/i2c@ff170000";
		mshc0 = "/dwmmc@ff0f0000";
		mshc1 = "/dwmmc@ff0c0000";
		mshc2 = "/dwmmc@ff0d0000";
		mshc3 = "/dwmmc@ff0e0000";
		serial0 = "/serial@ff180000";
		serial1 = "/serial@ff190000";
		serial2 = "/serial@ff690000";
		serial3 = "/serial@ff1b0000";
		serial4 = "/serial@ff1c0000";
		spi0 = "/spi@ff110000";
		spi1 = "/spi@ff120000";
		spi2 = "/spi@ff130000";
	};

	memory {
		device_type = "memory";
		reg = <0x0 0x80000000>;
	};

	arm-pmu {
		compatible = "arm,cortex-a12-pmu";
		interrupts = <0x0 0x97 0x4 0x0 0x98 0x4 0x0 0x99 0x4 0x0 0x9a 0x4>;
		interrupt-affinity = <0x2 0x3 0x4 0x5>;
	};

	cpus {
		#address-cells = <0x1>;
		#size-cells = <0x0>;
		enable-method = "rockchip,rk3066-smp";
		rockchip,pmu = <0x6>;

		cpu@500 {
			device_type = "cpu";
			compatible = "arm,cortex-a12";
			reg = <0x500>;
			resets = <0x7 0x0>;
			operating-points-v2 = <0x8>;
			#cooling-cells = <0x2>;
			clocks = <0x7 0x6>;
			nvmem-cells = <0x9>;
			nvmem-cell-names = "package_info";
			cpu0-supply = <0xa>;
			linux,phandle = <0x2>;
			phandle = <0x2>;
		};

		cpu@501 {
			device_type = "cpu";
			compatible = "arm,cortex-a12";
			reg = <0x501>;
			resets = <0x7 0x1>;
			operating-points-v2 = <0x8>;
			linux,phandle = <0x3>;
			phandle = <0x3>;
		};

		cpu@502 {
			device_type = "cpu";
			compatible = "arm,cortex-a12";
			reg = <0x502>;
			resets = <0x7 0x2>;
			operating-points-v2 = <0x8>;
			linux,phandle = <0x4>;
			phandle = <0x4>;
		};

		cpu@503 {
			device_type = "cpu";
			compatible = "arm,cortex-a12";
			reg = <0x503>;
			resets = <0x7 0x3>;
			operating-points-v2 = <0x8>;
			linux,phandle = <0x5>;
			phandle = <0x5>;
		};
	};

	opp_table0 {
		compatible = "operating-points-v2";
		opp-shared;
		linux,phandle = <0x8>;
		phandle = <0x8>;

		opp@126000000 {
			opp-supported-hw = <0x3>;
			opp-hz = <0x0 0x7829b80>;
			opp-microvolt = <0xdbba0>;
			clock-latency-ns = <0x9c40>;
		};

		opp@216000000 {
			opp-supported-hw = <0x3>;
			opp-hz = <0x0 0xcdfe600>;
			opp-microvolt = <0xdbba0>;
			clock-latency-ns = <0x9c40>;
		};

		opp@408000000 {
			opp-supported-hw = <0x3>;
			opp-hz = <0x0 0x18519600>;
			opp-microvolt = <0xdbba0>;
			clock-latency-ns = <0x9c40>;
		};

		opp@600000000 {
			opp-supported-hw = <0x3>;
			opp-hz = <0x0 0x23c34600>;
			opp-microvolt = <0xdbba0>;
			clock-latency-ns = <0x9c40>;
		};

		opp@696000000 {
			opp-supported-hw = <0x3>;
			opp-hz = <0x0 0x297c1e00>;
			opp-microvolt = <0xe7ef0>;
			clock-latency-ns = <0x9c40>;
		};

		opp@816000000 {
			opp-supported-hw = <0x3>;
			opp-hz = <0x0 0x30a32c00>;
			opp-microvolt = <0xf4240>;
			clock-latency-ns = <0x9c40>;
			opp-suspend;
		};

		opp@1008000000 {
			opp-supported-hw = <0x3>;
			opp-hz = <0x0 0x3c14dc00>;
			opp-microvolt = <0x100590>;
			clock-latency-ns = <0x9c40>;
		};

		opp@1200000000 {
			opp-supported-hw = <0x3>;
			opp-hz = <0x0 0x47868c00>;
			opp-microvolt = <0x10c8e0>;
			clock-latency-ns = <0x9c40>;
		};

		opp@1416000000 {
			opp-supported-hw = <0x3>;
			opp-hz = <0x0 0x54667200>;
			opp-microvolt = <0x124f80>;
			clock-latency-ns = <0x9c40>;
		};

		opp@1512000000 {
			opp-supported-hw = <0x3>;
			opp-hz = <0x0 0x5a1f4a00>;
			opp-microvolt = <0x13d620>;
			opp-microvolt-v1 = <0x1312d0>;
			clock-latency-ns = <0x9c40>;
		};

		opp@1608000000 {
			opp-supported-hw = <0x3>;
			opp-hz = <0x0 0x5fd82200>;
			opp-microvolt = <0x149970>;
			opp-microvolt-v1 = <0x13d620>;
			clock-latency-ns = <0x9c40>;
		};

		opp@1704000000 {
			opp-supported-hw = <0x3>;
			opp-hz = <0x0 0x6590fa00>;
			opp-microvolt = <0x149970>;
			opp-microvolt-v1 = <0x149970>;
			clock-latency-ns = <0x9c40>;
		};

		opp@1800000000 {
			opp-supported-hw = <0x3>;
			opp-hz = <0x0 0x6b49d200>;
			opp-microvolt = <0x149970>;
			opp-microvolt-v1 = <0x155cc0>;
			clock-latency-ns = <0x9c40>;
		};
	};

	amba {
		compatible = "arm,amba-bus";
		#address-cells = <0x1>;
		#size-cells = <0x1>;
		ranges;

		dma-controller@ff250000 {
			compatible = "arm,pl330", "arm,primecell";
			reg = <0xff250000 0x4000>;
			interrupts = <0x0 0x2 0x4 0x0 0x3 0x4>;
			#dma-cells = <0x1>;
			arm,pl330-broken-no-flushp;
			peripherals-req-type-burst;
			clocks = <0x7 0xc2>;
			clock-names = "apb_pclk";
			linux,phandle = <0x1a>;
			phandle = <0x1a>;
		};

		dma-controller@ff600000 {
			compatible = "arm,pl330", "arm,primecell";
			reg = <0xff600000 0x4000>;
			interrupts = <0x0 0x0 0x4 0x0 0x1 0x4>;
			#dma-cells = <0x1>;
			arm,pl330-broken-no-flushp;
			peripherals-req-type-burst;
			clocks = <0x7 0xc1>;
			clock-names = "apb_pclk";
			status = "disabled";
		};

		dma-controller@ffb20000 {
			compatible = "arm,pl330", "arm,primecell";
			reg = <0xffb20000 0x4000>;
			interrupts = <0x0 0x0 0x4 0x0 0x1 0x4>;
			#dma-cells = <0x1>;
			arm,pl330-broken-no-flushp;
			peripherals-req-type-burst;
			clocks = <0x7 0xc1>;
			clock-names = "apb_pclk";
			linux,phandle = <0x62>;
			phandle = <0x62>;
		};
	};

	reserved-memory {
		#address-cells = <0x1>;
		#size-cells = <0x1>;
		ranges;

		dma-unusable@fe000000 {
			reg = <0xfe000000 0x1000000>;
		};
	};

	oscillator {
		compatible = "fixed-clock";
		clock-frequency = <0x16e3600>;
		clock-output-names = "xin24m";
		#clock-cells = <0x0>;
		linux,phandle = <0xc>;
		phandle = <0xc>;
	};

	edp-phy {
		compatible = "rockchip,rk3288-dp-phy";
		clocks = <0x7 0x68>;
		clock-names = "24m";
		rockchip,grf = <0xb>;
		#phy-cells = <0x0>;
		status = "disabled";
		linux,phandle = <0x71>;
		phandle = <0x71>;
	};

	timer {
		compatible = "arm,armv7-timer";
		arm,cpu-registers-not-fw-configured;
		interrupts = <0x1 0xd 0xf04 0x1 0xe 0xf04 0x1 0xb 0xf04 0x1 0xa 0xf04>;
		clock-frequency = <0x16e3600>;
	};

	timer@ff810000 {
		compatible = "rockchip,rk3288-timer";
		reg = <0xff810000 0x20>;
		interrupts = <0x0 0x48 0x4>;
		clocks = <0xc 0x7 0x161>;
		clock-names = "timer", "pclk";
	};

	display-subsystem {
		compatible = "rockchip,display-subsystem";
		ports = <0xd 0xe>;
	};

	dwmmc@ff0c0000 {
		compatible = "rockchip,rk3288-dw-mshc";
		clock-freq-min-max = <0x61a80 0x8f0d180>;
		clocks = <0x7 0x1c8 0x7 0x44 0x7 0x72 0x7 0x76>;
		clock-names = "biu", "ciu", "ciu-drive", "ciu-sample";
		fifo-depth = <0x100>;
		interrupts = <0x0 0x20 0x4>;
		reg = <0xff0c0000 0x4000>;
		status = "okay";
		bus-width = <0x4>;
		cap-mmc-highspeed;
		cap-sd-highspeed;
		sd-uhs-sdr12;
		sd-uhs-sdr25;
		sd-uhs-sdr50;
		sd-uhs-sdr104;
		card-detect-delay = <0xc8>;
		disable-wp;
		num-slots = <0x1>;
		pinctrl-names = "default";
		pinctrl-0 = <0xf 0x10 0x11 0x12>;
		supports-sd;
		vmmc-supply = <0x13>;
		vqmmc-supply = <0x14>;
	};

	dwmmc@ff0d0000 {
		compatible = "rockchip,rk3288-dw-mshc";
		clock-freq-min-max = <0x30d40 0x2faf080>;
		clocks = <0x7 0x1c9 0x7 0x45 0x7 0x73 0x7 0x77>;
		clock-names = "biu", "ciu", "ciu-drive", "ciu-sample";
		fifo-depth = <0x100>;
		interrupts = <0x0 0x21 0x4>;
		reg = <0xff0d0000 0x4000>;
		status = "okay";
		clock-frequency = <0x2faf080>;
		bus-width = <0x4>;
		cap-sd-highspeed;
		cap-sdio-irq;
		disable-wp;
		keep-power-in-suspend;
		mmc-pwrseq = <0x15>;
		non-removable;
		num-slots = <0x1>;
		pinctrl-names = "default";
		pinctrl-0 = <0x16 0x17 0x18>;
		sd-uhs-sdr104;
		supports-sdio;
	};

	dwmmc@ff0e0000 {
		compatible = "rockchip,rk3288-dw-mshc";
		clock-freq-min-max = <0x61a80 0x8f0d180>;
		clocks = <0x7 0x1ca 0x7 0x46 0x7 0x74 0x7 0x78>;
		clock-names = "biu", "ciu", "ciu-drive", "ciu-sample";
		fifo-depth = <0x100>;
		interrupts = <0x0 0x22 0x4>;
		reg = <0xff0e0000 0x4000>;
		status = "disabled";
	};

	dwmmc@ff0f0000 {
		compatible = "rockchip,rk3288-dw-mshc";
		clock-freq-min-max = <0x61a80 0x8f0d180>;
		clocks = <0x7 0x1cb 0x7 0x47 0x7 0x75 0x7 0x79>;
		clock-names = "biu", "ciu", "ciu-drive", "ciu-sample";
		fifo-depth = <0x100>;
		interrupts = <0x0 0x23 0x4>;
		reg = <0xff0f0000 0x4000>;
		status = "disabled";
		supports-emmc;
	};

	saradc@ff100000 {
		compatible = "rockchip,saradc";
		reg = <0xff100000 0x100>;
		interrupts = <0x0 0x24 0x4>;
		#io-channel-cells = <0x1>;
		clocks = <0x7 0x49 0x7 0x15b>;
		clock-names = "saradc", "apb_pclk";
		status = "okay";
		vref-supply = <0x19>;
	};

	spi@ff110000 {
		compatible = "rockchip,rk3288-spi", "rockchip,rk3066-spi";
		clocks = <0x7 0x41 0x7 0x152>;
		clock-names = "spiclk", "apb_pclk";
		dmas = <0x1a 0xb 0x1a 0xc>;
		dma-names = "tx", "rx";
		interrupts = <0x0 0x2c 0x4>;
		pinctrl-names = "default";
		pinctrl-0 = <0x1b 0x1c 0x1d 0x1e>;
		reg = <0xff110000 0x1000>;
		#address-cells = <0x1>;
		#size-cells = <0x0>;
		status = "disabled";
	};

	spi@ff120000 {
		compatible = "rockchip,rk3288-spi", "rockchip,rk3066-spi";
		clocks = <0x7 0x42 0x7 0x153>;
		clock-names = "spiclk", "apb_pclk";
		dmas = <0x1a 0xd 0x1a 0xe>;
		dma-names = "tx", "rx";
		interrupts = <0x0 0x2d 0x4>;
		pinctrl-names = "default";
		pinctrl-0 = <0x1f 0x20 0x21 0x22>;
		reg = <0xff120000 0x1000>;
		#address-cells = <0x1>;
		#size-cells = <0x0>;
		status = "disabled";
	};

	spi@ff130000 {
		compatible = "rockchip,rk3288-spi", "rockchip,rk3066-spi";
		clocks = <0x7 0x43 0x7 0x154>;
		clock-names = "spiclk", "apb_pclk";
		dmas = <0x1a 0xf 0x1a 0x10>;
		dma-names = "tx", "rx";
		interrupts = <0x0 0x2e 0x4>;
		pinctrl-names = "default";
		pinctrl-0 = <0x23 0x24 0x25 0x26 0x27>;
		reg = <0xff130000 0x1000>;
		#address-cells = <0x2>;
		#size-cells = <0x0>;
		status = "okay";
		max-freq = <0x2faf080>;

		spidev@0 {
			compatible = "rockchip,spi_tinker";
			reg = <0x0>;
			spi-max-frequency = <0x2faf080>;
			spi-cpha = <0x1>;
		};

		spidev@1 {
			compatible = "rockchip,spi_tinker";
			reg = <0x1>;
			spi-max-frequency = <0x2faf080>;
			spi-cpha = <0x1>;
